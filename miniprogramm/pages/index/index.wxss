view {
  /* 主题颜色 */
  color: var(--themeColor);
}


.index_swiper swiper {

  width: 750rpx;
  height: 340rpx;

}

swiper navigator image {
  width: 100%;
  /* height:340rpx; */
}

.index_cate {
  display: flex;
}

.index_cate navigator {
  padding: 20rpx;
  flex: 1;
}

.index_cate navigator image {
  width: 100%;
}


.index_floor .floor_group .floor_title{
  padding: 10rpx 0;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.floor_list{
  overflow: hidden;
}
.floor_list navigator {
  float: left;
  width: 33.33%;
  /* 后四个超链接 */
}

/* 后四个超链接 
原图的宽高 232 * 386
323 / 386 = 33.33vw / height
*/
.index_floor .floor_group .floor_list navigator :nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list navigator :nth-child(2) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list navigator :nth-last-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}